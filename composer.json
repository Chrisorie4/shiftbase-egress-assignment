{
    "name": "shiftbase/egress",
    "description": "A package to quickly develop new API clients",
    "type": "library",
    "require": {
        "php": "^8.0",
        "psr/http-message": "^1.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "guzzlehttp/psr7": "^1.8",
        "http-interop/http-factory-guzzle": "^1.2",
        "guzzlehttp/guzzle": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Shiftbase\\Egress\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shiftbase\\Egress\\Tests\\": "tests/"
        }
    }
}
